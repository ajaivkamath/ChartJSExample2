<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
	<package name="struts2hibernate" extends="struts-default">
	
		<action name="chartData" class="com.charjsexample.Actions.ChartAction" method="getChartData">
			<result name="SUCCESS">/loadData.jsp</result>
		</action>

		<action name="chartPage" class="com.charjsexample.Actions.ChartAction" method="getChartData">
			<result name="SUCCESS">/chartPage.jsp</result>
		</action>

		<action name="createNewChart" class="com.charjsexample.Actions.ChartAction">
			<result name="SUCCESS">/createChartReport.jsp</result>
		</action>
	
	</package>

</struts>